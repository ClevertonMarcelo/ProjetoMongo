<div class="conteiner_listausuario">

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th scope="col">Login</th>
                <th scope="col">Senha</th>
                <th scope="col">Pergunta Secreta</th>
                <th scope="col">Resposta Secreta</th>
                <th scope="col">Editar</th>
                <th scope="col">Deletar</th>
            </tr>
        </thead>
        <tbody>
            {{#each usuarios}}
            <tr>
                <th scope="row">{{login}}</th>
                <td>{{senha}}</td>
                <td>{{pergunta_secreta}}</td>
                <td>{{resposta_pergunta}}</td>
                <td><a href="usuarioEdit/{{_id}}" class="btn btn-primary"> Editar </a></td>
                <td><a href="usuarioDelete/{{_id}}" class="btn btn-danger"> Deletar </a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>

</div>